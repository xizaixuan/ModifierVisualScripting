VseGraphView .stack-node .ge-port.execution #connector #cap {
    width: 8px;
    height: 8px;
    --port-color: #393939;
    border-color: #393939;
}

VseGraphView .ge-node .ge-port.execution-active {
    --port-color: #b4ff69;
}

VseGraphView .stack-node .ge-port.execution #type {
    margin-left: 0;
    margin-right: 0;
}

Port .unity-enum-field__text
{
   flex-grow: initial;
}

Port > .unity-property-field > FloatField
{
    margin: 0;
    padding: 0;
}
