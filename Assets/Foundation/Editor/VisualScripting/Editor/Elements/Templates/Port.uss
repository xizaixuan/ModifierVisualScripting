/**************/
/* Port icons */
/**************/
.token-node .port.output > #connector {
    margin-left: 4px;
}

.ge-port__icon {
    width: 12px;
    height: 12px;
    --unity-image: url("Icons/Advanced@x3.png");
}

.ge-node__inputs .ge-port__icon {
    margin-left: 4px;
}

.ge-node__outputs .ge-port__icon {
    margin-right: 4px;
}

.ge-port--data-type-execution-flow .ge-port__icon {
    display: none;
}

.ge-port--data-type-object .ge-port__icon {
    --unity-image: url("Icons/GraphicScript@x3.png");
}

.ge-port--data-type-texture2D .ge-port__icon {
    --unity-image: url("Icons/2DTexture@x3.png");
}

.ge-port--data-type-float4 .ge-port__icon,
.ge-port--data-type-vector4 .ge-port__icon {
    --unity-image: url("Icons/Vector4@x3.png");
}

